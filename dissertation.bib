  issn		= {1077-2626},
  journal	= {IEEE Transactions on Visualization and Computer Graphics},
  month		= jan,
  number	= {1},
  pages		= {611 -- 620},
  title		= {{W}eight{L}ifter: Visual Weight Space Exploration for
		  Multi-Criteria Decision Making},
  volume	= {23},
  year		= {2016}
}

